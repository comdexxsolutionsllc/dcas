{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "doctrine/dbal": "~2.3",
        "dompdf/dompdf": "*",
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "laravel/cashier": "~7.0",
        "laravel/envoy": "~1.0",
        "laravel/passport": "^1.0",
        "laravel/scout": "*",
        "laravel/socialite": "*",
        "laravelcollective/html": "5.3.*",
        "stripe/stripe-php": "~3.0",
        "kepex/laravel-crud-generator": "^0.1.4",
        "spatie/laravel-fractal": "^2.0.0",
        "league/fractal": "0.14.0",
        "graham-campbell/exceptions": "^9.1",
        "laracasts/presenter": "^0.2.1",
        "tightenco/quicksand": "^0.2.1",
        "codeception/codeception": "*",
        "mpociot/laravel-apidoc-generator": "^1.6",
        "laracasts/flash": "^2.0",
        "predis/predis": "^1.1",
        "nwidart/laravel-modules": "^1.13",
        "mirzabusatlic/laravel-schedule-monitor": "^1.01",
        "pulkitjalan/geoip": "2.4.*",
        "jrm2k6/cloudder": "0.3.*",
        "algolia/algoliasearch-client-php": "^1.10",
        "garygreen/pretty-routes": "^0.0.2",
        "yajra/laravel-datatables-oracle": "~6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "jlapp/swaggervel": "dev-master",
        "laracasts/generators": "^1.1",
        "filp/whoops": "^2.1",
        "phpspec/phpspec": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "DCASDomain\\": "app/DCASDomain/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ], 
        "files": [
            "tests/Helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
